<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record model="ir.ui.view" id="scm_raw_form_views">
        <field name="name">Raw form</field>
        <field name="model">scm.raw</field>
        <field name="arch" type="xml">
            <form string="Choose your date">
                <group>
                    <group>
                        <field name="end_date" required="1"/>
                    </group>
                </group>
                <footer>
                    <button name="get_raw_excel_report" string="XLSXexport" type="object" class="oe_highlight"/>
                    <button string="Cancel" class="btn-default" special="cancel"/>
                </footer>
            </form>
        </field>
    </record>

    <record model="ir.actions.act_window" id="scm_raw_action">
        <field name="name">Raw</field>
        <field name="res_model">scm.raw</field>
        <field name="view_mode">form</field>
        <field name="view_id" ref="scm_abc_raw.scm_raw_form_views"/>
        <field name="target">new</field>
    </record>
</odoo>